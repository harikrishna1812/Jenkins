pipeline {
    agent,{
        labels 'Agent2'
    }
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello world!' 
            }
        }
        stage('Stage 2') {
            steps {
                echo 'this is second stage!' 
            }
        }
        stage('Stage 3') {
            steps {
                echo 'This is third stage!' 
            }
        }
    }
}
